{
  "name": "tsl-react",
  "version": "0.0.3-next.13",
  "description": "A unified plugin that combines all individual plugins from the react-analyzer monorepo into one.",
  "keywords": [
    "react",
    "tsl",
    "tsl-react",
    "tsl-react-x",
    "tsl-react-dom",
    "tsl-react-web-api",
    "tsl-react-naming-convention"
  ],
  "license": "MIT",
  "author": "Rel1cx<rel1cx@proton.me>",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "./package.json"
  ],
  "scripts": {
    "build": "tsdown --dts-resolve",
    "build:docs": "typedoc",
    "lint:publish": "pnpm publint",
    "lint:ts": "tsl",
    "publish": "pnpm run build && pnpm run lint:publish"
  },
  "dependencies": {
    "@react-analyzer/ast": "workspace:*",
    "@react-analyzer/core": "workspace:*",
    "@react-analyzer/eff": "workspace:*",
    "@react-analyzer/kit": "workspace:*",
    "@react-analyzer/shared": "workspace:*",
    "compare-versions": "^6.1.1"
  },
  "devDependencies": {
    "@local/configs": "workspace:*",
    "tsdown": "^0.15.2"
  },
  "peerDependencies": {
    "tsl": "^1.0.24",
    "typescript": "^4.9.5 || ^5.4.5"
  },
  "engines": {
    "bun": ">=1.2.18",
    "node": ">=24.6.0"
  },
  "publishConfig": {
    "access": "public"
  }
}

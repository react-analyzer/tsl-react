{
  "extends": [
    "@tsconfig/strictest/tsconfig.json"
  ],
  "compilerOptions": {
    "noEmit": true,
    "erasableSyntaxOnly": true,
    "allowImportingTsExtensions": true,
    "types": [
      "tsl/patches"
    ],
    "plugins": [
      {
        "name": "tsl/plugin"
      },
      {
        "name": "@effect/language-service"
      }
    ]
  }
}

{
  "name": "@local/tsl-local",
  "version": "0.0.0",
  "private": true,
  "description": "Local tsl rules for use in the workspace",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "package.json"
  ],
  "scripts": {
    "build": "tsdown",
    "build:docs": "typedoc",
    "lint:publish": "publint",
    "lint:ts": "tsl"
  },
  "dependencies": {
    "@let/eff": "jsr:^0.1.2",
    "@react-analyzer/ast": "workspace:*",
    "@react-analyzer/shared": "workspace:*",
    "string-ts": "^2.3.1",
    "ts-pattern": "^5.9.0"
  },
  "devDependencies": {
    "@local/configs": "workspace:*",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "tsdown": "^0.19.0-beta.3"
  },
  "peerDependencies": {
    "tsl": "^1.0.28",
    "typescript": "^4.9.5 || ^5.4.5"
  },
  "engines": {
    "bun": ">=1.2.18",
    "node": ">=24.6.0"
  }
}
